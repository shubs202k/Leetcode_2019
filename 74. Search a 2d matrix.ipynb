    "from typing import List\n",
    "class Solution:\n",
    "    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:\n",
    "        flat_list = [item for sublist in matrix for item in sublist]\n",
    "        if target in flat_list:\n",
    "            return True\n",
    "        return False"
   ]
