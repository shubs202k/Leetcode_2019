    "from typing import List\n",
    "class Solution:\n",
    "    def combine(self, n: int, k: int) -> List[List[int]]:\n",
    "        from itertools import combinations\n",
    "        numbers = [num for num in range(1,n+1)]\n",
    "        return [list(x) for x in combinations(numbers,k)]"
   ]
