   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def findMaxAverage(self, nums: List[int], k: int) -> float:\n",
    "        candidates = [nums[char:char+k] for char in range(len(nums) - (k-1))]\n",
    "        return max([sum(element)/k for element in candidates])"
   ]
