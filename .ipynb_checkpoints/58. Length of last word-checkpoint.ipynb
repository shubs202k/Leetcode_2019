   "source": [
    "class Solution:\n",
    "    def lengthOfLastWord(self, s: str) -> int:\n",
    "        if not s:\n",
    "            return 0\n",
    "        if s.isspace():\n",
    "            return 0\n",
    "        length = len(s.split()[-1])\n",
    "        return length"
   ]
