    "from typing import List\n",
    "class Solution:\n",
    "    def searchInsert(self, nums: List[int], target: int) -> int:\n",
    "        if target in nums:\n",
    "            return nums.index(target)\n",
    "        nums.append(target)\n",
    "        nums.sort()\n",
    "        return nums.index(target)    "
   ]
