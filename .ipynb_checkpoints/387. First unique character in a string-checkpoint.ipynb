   "source": [
    "import collections\n",
    "class Solution:\n",
    "    def firstUniqChar(self, s: str) -> int:\n",
    "        if not s:\n",
    "            return -1\n",
    "        c = collections.Counter(s)\n",
    "        for i,char in enumerate(s):\n",
    "            if(c[char]==1):\n",
    "                return i\n",
    "        return -1"
   ]
