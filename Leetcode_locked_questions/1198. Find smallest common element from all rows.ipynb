   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def smallestCommonElement(self, mat: List[List[int]]) -> int:\n",
    "        result = set(mat[0])\n",
    "        for s in mat[1:]:\n",
    "            result.intersection_update(s)\n",
    "        if not result:\n",
    "            return -1\n",
    "        return min(result)"
   ]
